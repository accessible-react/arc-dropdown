{
  "name": "arc-dropdown",
  "version": "1.0.0",
  "description": "Accessible react dropdown",
  "main": "dist/bundle.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "NODE_ENV=development webpack-dev-server --hot --history-api-fallback",
